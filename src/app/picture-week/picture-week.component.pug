section#about
        .container
          .row
            h2.section-heading Picture of the Week
            #week.col-md-6
              .photo
                img.img-responsive(src='../assets/{{image}}', alt='{{title}}')
                a(href='../assets/{{image}}' data-imagelightbox="a")
                  .photo-overlay
                    h3 {{title}}
                    p Week of: {{week}}
                    p Location: {{location}}, {{state}}
                    p(style="text-align: center") {{description}}
            .col-md-6
              div(map-lazy-load='https:/maps.googleapis.com/maps/api/js?key=AIzaSyBrpgMWBTZRs58bzHOzCfESQPsvI09zOuo&v=3')